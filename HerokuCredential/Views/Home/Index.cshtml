<form>
    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input type="email" class="form-control" id="emaildata" aria-describedby="emailHelp" placeholder="Enter email">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" id="passworddata" placeholder="Password">
    </div>
    
</form>
    <button class="btn btn-primary" onclick="Login()">Submit</button>
    <button class="btn btn-secondary" onclick="Create()">Create account</button>
<hr/>

<div id="PartialLoginResult">
    
</div>

<script>
    function Login() {
        var email = document.getElementById("emaildata").value;
        console.log("email: " + email);
        var password = document.getElementById("passworddata").value;
        console.log("password: " + password);
        $.ajax({
          url: "/Home/Login",
          type: 'POST',
          data: {
              "email": email,
              "password": password
            },
          
          success: function (result) {
              $('#PartialLoginResult').html(result);
              console.log(result);
            
          },
          error: function (error) {
              console.log(error);
          }
        });
        }       

    function Create() {
        var email = document.getElementById("emaildata").value;
        console.log("email: " + email);
        var password = document.getElementById("passworddata").value;
        console.log("password: " + password);
        $.ajax({
          url: "/Home/Create",
          type: 'POST',
          data: {
              "email": email,
              "password": password
            },
          
          success: function (result) {
              $('#PartialLoginResult').html(result);
              console.log(result);
            
          },
          error: function (error) {
              console.log(error);
          }
        });
    }       
</script>
